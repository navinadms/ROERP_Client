                txtPlantName.Text = Convert.ToString(dataOrder(0).Plant)
                txttentiveSchem.Text = Convert.ToString(dataOrder(0).TreatmentScheme)

            End If

            Dim partyDetail = linq_obj.SP_Select_Party_Master_ByAddressId(Address_ID).ToList()
